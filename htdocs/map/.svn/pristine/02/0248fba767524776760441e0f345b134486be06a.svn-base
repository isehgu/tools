/*For shrinking effect*/
div.shrink {
  width: 50%;
}
div.shrink>h1{
  font-size:20px;
}
div.shrink>ul{
  font-size:6px;
}
/******************************************************************/
.header_stat,.header_text>h3,.header_stat>h3,.header_text>h1,.header_text>h2{
  color: black;
  font-family: 'Syncopate';
  vertical-align: middle;
}
.header_stat>ul {
