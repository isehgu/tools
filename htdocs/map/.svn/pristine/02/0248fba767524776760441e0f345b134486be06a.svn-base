/****************************************************************/
/* Event History (only some) */
/****************************************************************/
.wide-row{
  max-width: 90%;
}
/****************************************************************/
/* Test History */
/****************************************************************/
.test_result_icon,.test_replay {
  font-size: 32px;
  cursor: pointer;
}
.test_history_row{
  cursor: pointer;
}
.test_result_icon_null{
  font-size: 32px;
  color: #D8D8D8 !important;
}

.test_replay:hover{
  color:#FE9A2E;
  font-size: 38px;
}
.template_stying{
  margin-top: 50px;
  border-radius: 6px 6px 6px 6px;
  background-color: rgba(237, 237, 237, 1);/*#cccccc !important;*/
}
/****************************************************************/
/* Task */
/****************************************************************/
.cancel_cross{
  font-size:26px !important;
  color:#E33100 !important;
}
.cancel_cross:hover{
  cursor: pointer;
  text-shadow: 0 0 5px #fff,0 0 10px #fff,0 0 15px #fff,0 0 20px #E33100,0 0 30px #E33100,0 0 40px #E33100;
}
.disabled_cancel_cross{
  font-size:26px !important;
  color:#D8D8D8 !important;
}
.task_test_pending{
  display: inline;
  font-weight: normal;
}
/****************************************************************/
/* Live Test */
/****************************************************************/
.checkbox_td{
  padding-top:25px;
  padding-left:20px;
}

/****************************************************************/
/* User Section in top-bar */
/****************************************************************/
.top_bar_error{
  color: red;
  line-height: 45px;
  font-size: 1rem;
  text-align: center;
  font-family: "Helvetica Neue", "Helvetica", Helvetica, Arial, sans-serif;
  /*background-color: #ff0000;*/
 /* overflow: hidden;*/
}

input[type="text"].input-validation-error1{
    border: 1px solid #ff0000;
    background-color: #ffeeee;
}
input[type="password"].input-validation-error1{
    border: 1px solid #ff0000;
    background-color: #ffeeee;
}
input[type="text"].input-validation-error2{
    /*border: 1px solid #ff0000;*/
    /*background-color: #ffeeee;*/
    border: 1px solid #ff0000;
    background-color: transparent;
    color: red;
}
input[type="password"].input-validation-error2{
    /*border: 1px solid #ff0000;*/
    /*background-color: #ffeeee;*/
    border: 1px solid #ff0000;
    background-color: transparent;
    color: red;
}

input[type="text"].input-styling{
    color: white;
    background-color: transparent;
    border-style: solid;
    border-width: 0px 0px 1px 0px;
    /*border-color: #008cba;*/
    border-color: #A4A4A4;
    outline:0;
}

input[type="password"].input-styling{
    color: white;
    background-color: transparent;
    border-style: solid;
    border-width: 0px 0px 1px 0px;
    /*border-color: #008cba;*/
    border-color: #A4A4A4;
    outline:0;
}
::-webkit-input-placeholder { /* WebKit browsers */
    /*color: #00C0FF;*/
    color: #A4A4A4;
    text-align: center;
}

#top_bar_username_div, #top_bar_password_div, #top_bar_login_div {
  padding-left: 5px;
  /*display: block;*/
}
