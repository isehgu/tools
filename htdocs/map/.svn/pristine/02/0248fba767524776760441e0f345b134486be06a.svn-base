#toast_div{
  position: fixed;
  top: 100px;
  right: 20px;
  text-align: center;
  color: white;
  border: 1px solid #60635F; /*#17BD00;*/
  border-radius: 3px;
  background-color: #60635F; /*#CDFCC7;*/
  opacity: 0.9;
  width: 300px;
  /*height: 100px;*/
  display: none;
  padding-bottom: 15px;
}

