/* Google Font */
/****************************************************************/
html {
    /* Always show the scrollbar to avoid jarring sideways movement when data appears/disappears */
    overflow-y: scroll;
}
body{

  font-family: "Roboto","Helvetica Neue", "Helvetica", Helvetica, Arial, sans-serif;
}
#content{
  margin-top: 35px;
}
#appname{
  font-size: 28px !important;
  font-family: "Aclonica","Helvetica Neue", "Helvetica", Helvetica, Arial, sans-serif;
}
.myfont{
 font-family: Roboto !important;
}
.sub-nav {
  background-color: #f2f2f2;
  /*padding: 15px 10px 10px 0;*/
  padding-top: 10px;
  width:auto;
  margin-top: 0px;
  margin-left:0px;

  /*-webkit-animation: slidein 1s;
  animation: slidein 1s;*/
  display:none;
}
.row{
  /*max-width: 90% !important;*/
}
.section_header{
/*  border-radius: 5px 5px 5px 5px;
  -moz-border-radius: 5px 5px 5px 5px;
  -webkit-border-radius: 5px 5px 5px 5px;*/
  border-bottom: 1px solid #A6A6A6;
  /*background-color: #296eab;*/
  background-color: white;
  padding-top:15px;
  padding-bottom: 0px;
  margin-bottom: 0px;

  -webkit-transition: all 0.3s;
  -moz-transition: all 0.3s;
  -ms-transition: all 0.3s;
  -o-transition: all 0.3s;
  transition: all 0.3s
}
.section_header:hover{
  cursor: pointer;
  background-color: rgba(0,140,186,0.6); /*#008cba; rgba(131,195,252,0.5); /*#5182ad;*/*/
}

/*For shrinking effect*/
div.shrink {
  width: 50%;
}
div.shrink>h1{
  font-size:20px;
}
div.shrink>ul{
  font-size:6px;
}
/******************************************************************/
.header_stat,.header_text>h3,.header_stat>h3,.header_text>h1,.header_text>h2{
  color: black;
  font-family: 'Syncopate';
  vertical-align: middle;
}
.header_stat>ul {
  font-size: 12px;
  font-weight: bold;
}
.header_text>h1, .header_text>h3{
  display: inline;
}
.section_body{
  display:none;
}
.event_history_template_row:hover, .event_history_release_row:hover {
  cursor: pointer;
  background-color: rgba(0,140,186,0.6); /*#008cba; rgba(131,195,252,0.5); /*#5182ad;*/*/
}
.modal_content{
  margin-top:10px;
}
.font_syncopate{
  font-family: 'Syncopate';
}
.generic_table_row:hover{
  background-color: rgba(0,140,186,0.4);/*#cccccc !important;*/
}
/*template and template instance specific*/
.btn_td{
  padding-top: 20px;
  text-align: center;
}
button.disabled, button[disabled], .button.disabled, .button[disabled]{
  opacity: 0.3 !important;
}
.no_style_list li{
  list-style-type:none;
  display:inline;
  padding-right: 15px;
}
.strong{
  font-weight: bold !important;
}
/*.second_bar_link{
  color: #757575 !important;
}
*/
.progress{
  margin-bottom: 0px;
  font-size: 0.75rem;
  font-weight: normal;
}
.meter{
  text-align: right;
}

.wide-row{
  max-width: 90%;
}

